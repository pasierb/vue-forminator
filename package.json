{
  "name": "vue-forminator",
  "version": "1.0.0-beta.2",
  "private": false,
  "description": "Vue.js schema based form generator",
  "main": "./lib/vue-forminator.js",
  "homepage": "https://github.com/pasierb/vue-forminator",
  "repository": {
    "type": "git",
    "url": "https://github.com/pasierb/vue-forminator.git"
  },
  "bugs": {
    "url": "https://github.com/pasierb/vue-forminator/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "form",
    "generator",
    "json",
    "schema"
  ],
  "author": "mpasierbski",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "webpack",
    "build:docs": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "precommit": "npm run test",
    "prepublish": "npm run test && npm run build",
    "test": "npm run lint lint && jest"
  },
  "files": [
    "lib/**/*",
    "src/**/*",
    "*.json",
    "*.js",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "vue": ">= 2.3 < 3"
  },
  "dependencies": {
    "vue-functional-data-merge": "^2.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/server-test-utils": "^1.0.0-beta.20",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.4.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.4.0",
    "jest-serializer-vue": "^2.0.2",
    "pre-commit": "^1.2.2",
    "vue": "^2.3",
    "vue-template-compiler": "^2.5.16",
    "vuelidate": "^0.7.4",
    "webpack-cli": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:jest/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
